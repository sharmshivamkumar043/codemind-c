#include <bits/stdc++.h> 
using namespace std;   
int main() 
{ 
    int n;
    cin>>n;
    int sum=0, count=0, i;
    float avg;
    int a[n];
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
    }
    for(i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    avg=(float)sum/n;
    cout<<sum<<" "<<avg<<endl;
} 
